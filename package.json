  "scripts": {
    "start": "node dist/index.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:run": "nodemon --watch dist --exec \"yarn start\"",
    "dev:run-server": "ts-node ./src/server/index.ts",
    "dev:run-server:watch": "nodemon --watch ./src/server --exec \"yarn dev:run-server\"",
    "dev:build-server": "NODE_ENV=development webpack --config webpack.config.server.js --watch",
    "dev:build-client": "NODE_ENV=development webpack --config webpack.config.client.js --watch",
    "build": "npm-run-all --parallel prod:*",
    "prod:build-server": "NODE_ENV=production webpack --config webpack.config.server.js",
    "prod:build-client": "NODE_ENV=production webpack --config webpack.config.client.js",
    "test": ""
  },
